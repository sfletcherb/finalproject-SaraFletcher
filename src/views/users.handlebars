<div class="user-table-container">
  <h2>Usuarios Activos</h2>
  <table>
    <thead>
      <tr>
        <th>Nombre Completo</th>
        <th>Role</th>
        <th>Correo</th>
        <th>Ult.Conexi√≥n</th>
        <th>Acciones</th>
      </tr>
    </thead>
    <tbody>
      {{#each usersData}}
        <tr id="user-row-{{this.id}}">
          <td>{{this.first_name}} {{this.last_name}}</td>
          <td>{{this.role}}</td>
          <td>{{this.email}}</td>
          <td>{{this.last_connection}}</td>
          <td><button
              class="delete-btn"
              onclick="deleteUser('{{this.id}}')"
            >Eliminar</button></td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>

<script src="js/main.js"></script>